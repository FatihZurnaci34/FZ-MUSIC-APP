@model FZMusicEntites.Models.User
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title -->
    <title>FZ MUSIC KAYIT OL</title>

    <!-- Favicon -->
    <link rel="icon" href="~/one-music-gh-pages/img/core-img/favicon.ico">

    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/one-music-gh-pages/style.css">

</head>

<body>
    <!-- Preloader -->
    <div class="preloader d-flex align-items-center justify-content-center">
        <div class="lds-ellipsis">
            <div></div>
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>

    <!-- ##### Header Area Start ##### -->
    <header class="header-area">
        <!-- Navbar Area -->
        <div class="oneMusic-main-menu">
            <div class="classy-nav-container breakpoint-off">
                <div class="container">
                    <!-- Menu -->
                    <nav class="classy-navbar justify-content-between" id="oneMusicNav">

                        <!-- Nav brand -->
                        <a href="Index" class="nav-brand"><img src="~/one-music-gh-pages/img/core-img/FZ MUSIC1.png" style="width:80px; height:80px;" alt=""></a>

                        <!-- Navbar Toggler -->
                        <div class="classy-navbar-toggler">
                            <span class="navbarToggler"><span></span><span></span><span></span></span>
                        </div>

                        <!-- Menu -->
                        <div class="classy-menu">

                            <!-- Close Button -->
                            <div class="classycloseIcon">
                                <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                            </div>

                            <!-- Nav Start -->
                            <div class="classynav">
                                <ul>
                                    <li><a href="Index">Ana Sayfa</a></li>
                                    <li><a href="Contact">İletişim</a></li>
                                </ul>

                                <!-- Login/Register & Cart Button -->
                                <div class="login-register-cart-button d-flex align-items-center">
                                    <!-- Login/Register -->

                                    <ul>
                                        <li><a href="/Login/Index" id="loginBtn">Giriş</a></li>
                                        <li><a href="/Home/Register" id="registerBtn">Kayıt Ol</a></li>
                                    </ul>

                                </div>
                            </div>
                            <!-- Nav End -->

                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ##### Header Area End ##### -->
    <!-- ##### Breadcumb Area Start ##### -->
    <section class="breadcumb-area bg-img bg-overlay" style="background-image: url(../../one-music-gh-pages/img/bg-img/breadcumb3.jpg);">
        <div class="bradcumbContent">
            <!-- ##### Login Area Start ##### -->
            <img src="~/one-music-gh-pages/img/core-img/FZ MUSIC1.png" style="width:80px; height:80px;" alt=""> <h2>FZ Music</h2>
        </div>
    </section>
    <!-- ##### Breadcumb Area End ##### -->
    <!-- ##### Login Area Start ##### -->
    <section class="login-area section-padding-100">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 col-lg-8">
                    <div class="login-content" style="background-color:white;">
                        <h3>Dinlemeye başlamak için kaydol</h3>
                        <!-- Login Form -->
                        <div class="login-form">
                            <form action="/Home/Register/" method="post">
                                <div class="form-group">
                                    <label for="exampleInputEmail1">E-Posta Adresinizi Giriniz</label>
                                    <input type="email" class="form-control" name="Email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Parola Oluşturunuz</label>
                                    <input type="password" class="form-control"name="Password" id="exampleInputPassword1" placeholder="">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Tekrar Parola Giriniz</label>
                                    <input type="password" class="form-control" name="Password" id="exampleInputPassword1" placeholder="">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">İsminizi Giriniz</label>
                                    <input type="text" class="form-control" name="Name" aria-describedby="emailHelp" placeholder="">
                                </div>

                                <button type="submit" class="btn oneMusic-btn mt-30" style="margin-left:120px;">Kaydol</button>

                                <div style="margin-top:100px;"><h3>Hesabın varmı?<a href="Login" style="font-family:'Times New Roman';font-size:23px; color :#cc8400;">   Oturum Aç</a></h3></div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Login Area End ##### -->
    <!-- ##### Footer Area Start ##### -->
    <footer class="footer-area">
        <div class="container">
            <div class="row d-flex flex-wrap align-items-center">
                <div class="col-12 col-md-6">
                    <a href="Index"><img src="~/one-music-gh-pages/img/core-img/FZ MUSIC1.png" style="width:100px; height:100px;" alt=""></a>
                    <p class="copywrite-text">
                        <a href="#">
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            <script>document.write(new Date().getFullYear());</script> FZ COMMUNITY <i class="fa fa-heart-o" aria-hidden="true"></i> by Fatih
                        </a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>

                <div class="col-12 col-md-6">
                    <div class="footer-nav">
                        <ul>
                            <li><a href="Index">Ana Sayfa</a></li>
                            <li><a href="Contact">İletişim</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- ##### Footer Area Start ##### -->
    <!-- ##### All Javascript Script ##### -->
    <!-- jQuery-2.2.4 js -->
    <script src="~/one-music-gh-pages/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="~/one-music-gh-pages/js/bootstrap/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/one-music-gh-pages/js/bootstrap/bootstrap.min.js"></script>
    <!-- All Plugins js -->
    <script src="~/one-music-gh-pages/js/plugins/plugins.js"></script>
    <!-- Active js -->
    <script src="~/one-music-gh-pages/js/active.js"></script>
</body>

</html>