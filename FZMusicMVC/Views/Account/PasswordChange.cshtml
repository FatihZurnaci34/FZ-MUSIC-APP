@model FZMusicEntites.Models.User


@{
    ViewBag.Title = "PasswordChange";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@*<h2 style="color:aliceblue; text-align:center">Hoşgeldin @Session["Name"].ToString() Şifreni Değiştirmek İçin Yeni Şifrenizi Giriniz</h2>*@

<div style="text-align: center; margin: 15px; background-color: #343A40; color: white ">
<h2> <b>Şifre Değiştirme </b></h2>
</div>
<div class="tab-pane">
    <form class="form-horizontal" style="text-align:right" method="post">

        <div class="form-group row">
            <div class="col-sm-1"></div>
            <label for="inputName" class="col-sm-1 col-form-label" style="color:aliceblue; text-align:center;">Yeni Şifreniz</label>
            <div class="col-sm-8">
                <input name="Password" value="@Model.Password" class="form-control" id="inputName" placeholder="Yeni Şifrenizi Giriniz">
            </div>
            <div class="col-sm-2"></div>
        </div>

        <div class="form-group row" style="text-align:left">
            <div class="offset-sm-2 col-sm-10">
                <button type="submit" class="btn btn-default">Kaydet</button>
            </div>
        </div>

    </form>
</div>

